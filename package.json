{
  "name": "tender-flow",
  "private": true,
  "version": "1.2.10",
  "type": "module",
  "main": "desktop/dist/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:user-manual": "node scripts/build-user-manual.mjs",
    "preview": "vite preview",
    "start": "node server.js",
    "mcp": "cd mcp-bridge-server && npm start",
    "mcp:install": "cd mcp-bridge-server && npm install",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "desktop:install": "cd desktop && npm install --ignore-scripts",
    "desktop:compile": "cd desktop && tsc",
    "desktop:dev": "concurrently -k \"npm run dev -- --host 127.0.0.1 --port 3000\" \"wait-on tcp:3000 && npm run desktop:compile && electron .\"",
    "desktop:build": "ELECTRON_BUILD=true npm run build && npm run desktop:compile && electron-builder",
    "desktop:build:mac": "npm run desktop:build -- --mac",
    "desktop:build:win": "npm run desktop:build -- --win",
    "version:patch": "npm version patch --no-git-tag-version && node scripts/sync-version.mjs",
    "version:minor": "npm version minor --no-git-tag-version && node scripts/sync-version.mjs",
    "version:major": "npm version major --no-git-tag-version && node scripts/sync-version.mjs",
    "release:prepare": "node scripts/prepare-release.mjs",
    "deploy:stripe-webhook": "npx supabase functions deploy stripe-webhook --no-verify-jwt",
    "deploy:stripe-sync": "npx supabase functions deploy stripe-sync-subscription"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.24.1",
    "@netlify/functions": "^4.2.0",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.16",
    "@types/papaparse": "^5.5.0",
    "busboy": "^1.6.0",
    "dompurify": "^3.3.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.3.9",
    "exceljs": "^4.4.0",
    "express": "^4.22.1",
    "fflate": "^0.8.2",
    "html2canvas": "^1.4.1",
    "jspdf": "4.0.0",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.561.0",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.530",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.4.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/busboy": "^1.5.4",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^22.19.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "baseline-browser-mapping": "^2.9.17",
    "electron": "40.0.0",
    "electron-builder": "^26.5.0",
    "icon-gen": "^5.0.0",
    "jsdom": "^26.1.0",
    "marked": "^12.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.16",
    "wait-on": "^8.0.5"
  }
}
